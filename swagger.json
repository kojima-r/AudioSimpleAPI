            "properties":{
                "name":{
                    "type":"string",
                    "description":"\u5b9a\u4f4d\u306e\u8a2d\u5b9a\u306b\u5bfe\u3059\u308b\u540d\u524d",
                    "example":"default"
                },
                "audio_id":{
                    "type":"string",
                    "description":"\u5bfe\u8c61\u306eaudio_id",
                    "example":"0"
                },
                "transfer_function":{
                    "type":"string",
                    "description":"\u4f1d\u9054\u95a2\u6570\u30d5\u30a1\u30a4\u30eb\u540d",
                    "example":"0"
                },
                "src_num":{
                    "type":"integer",
                    "description":"\uff08\u5b9a\u4f4d\u30d1\u30e9\u30e1\u30fc\u30bf\uff09\u97f3\u6e90\u6570",
                    "example":"3"
                },
                "threshold":{
                    "type":"number",
                    "description":"\uff08\u5b9a\u4f4d\u30d1\u30e9\u30e1\u30fc\u30bf\uff09\u30b9\u30ec\u30c3\u30b7\u30e7\u30eb\u30c9",
                    "example":"28.5"
                },
                "lowest_freq":{
                    "type":"number",
                    "description":"\uff08\u5b9a\u4f4d\u30d1\u30e9\u30e1\u30fc\u30bf\uff09\u6700\u5c0f\u5468\u6ce2\u6570",
                    "example":"2200"
                },
                "pause_length":{
                    "type":"number",
                    "description":"\uff08\u5b9a\u4f4d\u30d1\u30e9\u30e1\u30fc\u30bf\uff09\u7a7a\u767d\u671f\u9593",
                    "example":"1200"

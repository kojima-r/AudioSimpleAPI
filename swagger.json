        },
        {
            "name":"trim",
            "description":"Trim\u306e\u30a8\u30f3\u30c9\u30dd\u30a4\u30f3\u30c8:\u5207\u308a\u51fa\u3057"
        },
        {
            "name":"localization",
            "description":"Localization\u306e\u30a8\u30f3\u30c9\u30dd\u30a4\u30f3\u30c8"
        },
        {
            "name":"separation",
            "description":"Separation\u306e\u30a8\u30f3\u30c9\u30dd\u30a4\u30f3\u30c8"
        },
        {
            "name":"util",
            "description":"utility\u306e\u30a8\u30f3\u30c9\u30dd\u30a4\u30f3\u30c8"
        }
    ],
    "definitions":{
        "Scene":{
            "required":[
                "audio_id",
                "event_list",
                "name"
            ],
            "properties":{
                "audio_id":{
                    "type":"string",
                    "description":"\u5207\u308a\u51fa\u3057\u305faudio_id",
                    "example":"1"
                },
                "event_list":{
                    "type":"array",
                    "items":{
                        "$ref":"#\/definitions\/Event"
                    }
                },
                "name":{
                    "type":"string",
                    "description":"\u5207\u308a\u51fa\u3057\u90e8\u5206\u306e\u540d\u524d",
                    "example":"trim A"
                }
            },
            "type":"object"
        },
        "Event":{
            "required":[
                "begin_time",
                "end_time",
                "point_list"
            ],
            "properties":{
                "begin_time":{
                    "type":"number",
                    "description":"\u30a4\u30d9\u30f3\u30c8\u306e\u958b\u59cb(sec) \u3001audio_id\u306e\u958b\u59cb\u30920\u79d2\u3068\u3059\u308b",
                    "example":"0"
                },
                "end_time":{
                    "type":"number",
                    "description":"\u30a4\u30d9\u30f3\u30c8\u306e\u7d42\u4e86(sec)\u3001audio_id\u306e\u958b\u59cb\u30920\u79d2\u3068\u3059\u308b",
                    "example":"0"
                },
                "sep_audio_id":{
                    "type":"number",
                    "description":"\u5206\u96e2\u97f3\u306e\u97f3\u58f0\u30d5\u30a1\u30a4\u30eb\u306eID",
                    "example":"0"
                },
                "point_list":{
                    "type":"array",
                    "items":{
                        "$ref":"#\/definitions\/EventPoint"
                    }
                },
                "label":{
                    "type":"string",
                    "description":"\u30a4\u30d9\u30f3\u30c8\u306e\u540d\u524d",
                    "example":"Event A"
                }
            },
            "type":"object"
        },
        "EventPoint":{
            "required":[
                "begin_time",
                "direction",
                "duration"
            ],
            "properties":{
                "begin_time":{
                    "type":"number",
                    "description":"\u5b9a\u4f4d\u306e\u958b\u59cb(sec) \u3001event\u306e\u958b\u59cb\u30920\u79d2\u3068\u3059\u308b",
                    "example":"0"
                },
                "duration":{
                    "type":"number",
                    "description":"\u5b9a\u4f4d\u6642\u9593(sec) \u3001\u30c7\u30d5\u30a9\u30eb\u30c8\u306e\u5b9a\u4f4d\u8a2d\u5b9a\u3060\u3068500msec\u3054\u3068\u306b\u5b9a\u4f4d\u3092\u884c\u3046",
                    "example":"0.5"
                },
                "direction":{
                    "type":"number",
                    "description":"\u5b9a\u4f4d\u65b9\u5411(degree) \u30010\u5ea6\u3092\u4e2d\u5fc3+180\u304b\u3089-180\u3067\u6307\u5b9a",
                    "example":"0"
                }
            },
            "type":"object"
        },
        "Trim":{
            "required":[
                "audio_id",
                "begin_time",
                "end_time",
                "original_audio_id"
            ],
            "properties":{
                "original_audio_id":{
                    "type":"string",
                    "description":"\u5143\u306e\u97f3\u58f0\u30d5\u30a1\u30a4\u30ebID\/\u30d5\u30a1\u30a4\u30eb\u30d1\u30b9",
                    "example":"0"
                },
                "begin_time":{
                    "type":"number",
                    "description":"\u5207\u308a\u51fa\u3057\u306e\u958b\u59cb(sec)\u3001original_audio_id\u306b\u5bfe\u5fdc\u3057\u305f\u97f3\u58f0\u30d5\u30a1\u30a4\u30eb\u306e\u958b\u59cb\u30920\u79d2\u3068\u3059\u308b",
                    "example":"0"
                },
                "end_time":{
                    "type":"number",
                    "description":"\u5207\u308a\u51fa\u3057\u306e\u7d42\u4e86(sec)\u3001original_audio_id\u306b\u5bfe\u5fdc\u3057\u305f\u97f3\u58f0\u30d5\u30a1\u30a4\u30eb\u306e\u958b\u59cb\u30920\u79d2\u3068\u3059\u308b",
                    "example":"0"
                },
                "audio_id":{
                    "type":"string",
                    "description":"\u5207\u308a\u51fa\u3057\u305faudio_id\/\u30d5\u30a1\u30a4\u30eb\u30d1\u30b9",
                    "example":"1"
                },
                "name":{
                    "type":"string",
                    "description":"\u5207\u308a\u51fa\u3057\u305f\u90e8\u5206\u306e\u540d\u524d",
                    "example":"trimA"
                }
            },
            "type":"object"
        },
        "TrimResponse":{
            "properties":{
                "name":{
                    "type":"string",
                    "description":"\u5207\u308a\u51fa\u3057\u305f\u90e8\u5206\u306e\u540d\u524d",
                    "example":"trimA"
                },
                "worker_id":{
                    "type":"integer",
